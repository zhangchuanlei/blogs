    <Sql><![CDATA[
    select * from (
Select log_main.*,log_main.user_id FLD_4A_ACCT,'' FLD_BOMC_ACCT From
(Select * From utoptea.tl_tt_log_4a log4a  Where Exists (Select * From ftoptea.ng_resm_item_acct Where fld_audit_status = 'ÒÑÉóºË' and FLD_4A_ACCT = log4a.user_id) and user_ip is not Null) log_main
Union All
Select log_sub.*,acct.FLD_4A_ACCT FLD_4A_ACCT,decode(acct.FLD_BOMC_ACCT ,Null,log_sub.user_id,acct.FLD_BOMC_ACCT) FLD_BOMC_ACCT From
(Select * From utoptea.tl_tt_log_4a log4a Where Not Exists (Select * From ftoptea.ng_resm_item_acct Where fld_audit_status = 'ÒÑÉóºË' and FLD_4A_ACCT = log4a.user_id) and user_ip is not Null) log_sub Left Join ftoptea.ng_resm_item_acct acct On Log_sub.user_id = acct.FLD_BOMC_ACCT
) order by operate_time
 ]]></Sql>
